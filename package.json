{
  "name": "@zuks/monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.7.1",
  "description": "Collection of essential Vue Component Library Composition Utilities",
  "author": "savoygu<https://github.com/savoygu>",
  "license": "MIT",
  "scripts": {
    "build": "nr update && nr zuks build",
    "build:rollup": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild",
    "build:types": "tsc  --emitDeclarationOnly",
    "clean": "rimraf dist types \"packages/*/dist\"",
    "dev": "nr update && nr docs:dev",
    "docs:dev": "vitepress dev packages --open",
    "docs:build": "nr update:full && vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "lint": "eslint --cache .",
    "publish:ci": "nr zuks publish",
    "release": "bumpp --execute=\"nr release:prepare\" --all",
    "release:prepare": "nr update:full",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "update": "nr -C packages/metadata update && nr zuks update",
    "update:full": "nr update && nr build:types",
    "watch": "nr zuks build --watch",
    "zuks": "esno scripts/main.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@anu-vue/preset-theme-default": "^0.15.2",
    "@iconify-json/bx": "^1.1.7",
    "@iconify/json": "^2.2.110",
    "@rollup/plugin-json": "^6.0.0",
    "@ruabick/md-demo-plugins": "^0.3.3",
    "@ruabick/vitepress-demo-block": "^0.3.3",
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.13",
    "@vueuse/core": "^10.4.1",
    "@vueuse/metadata": "^10.4.1",
    "@vueuse/shared": "^10.4.1",
    "@zuks/core": "workspace:*",
    "@zuks/metadata": "workspace:*",
    "@zuks/shared": "workspace:*",
    "anu-vue": "^0.15.2",
    "bumpp": "^9.2.0",
    "citty": "^0.1.3",
    "consola": "^3.2.3",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.48.0",
    "esno": "^0.17.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "fuse.js": "^6.6.2",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "lodash-es": "^4.17.21",
    "ohmyfetch": "^0.4.21",
    "pnpm": "^8.7.1",
    "postcss": "^8.4.29",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "simple-git": "^3.19.1",
    "simple-git-hooks": "^2.9.0",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2",
    "unocss": "^0.55.4",
    "unplugin-icons": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-inspect": "^0.7.38",
    "vitepress": "1.0.0-rc.10",
    "vitest": "^0.34.3",
    "vue": "^3.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
