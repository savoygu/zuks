    "docs:preview": "vitepress preview packages"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@antfu/ni": "^0.21.3",
    "@antfu/utils": "^0.7.4",
    "@types/node": "^18.16.16",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitepress": "1.0.0-beta.5",
    "vitest": "^0.31.3",
    "vue": "^3.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
