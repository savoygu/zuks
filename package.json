{
  "name": "@zuks/monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "description": "Collection of essential Vue Component Library Composition Utilities",
  "author": "savoygu <savoygu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache .",
    "release": "bumpp && npm publish",
