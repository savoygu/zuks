{
  "name": "@zuks/monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "description": "Collection of essential Vue Component Library Composition Utilities",
  "author": "savoygu<https://github.com/savoygu>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache .",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "nr update && nr docs:dev",
    "docs:dev": "vitepress dev packages --open",
    "docs:build": "nr update && vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "update:metadata": "nr -C packages/metadata update",
    "update": "nr update:metadata"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@antfu/ni": "^0.21.3",
    "@antfu/utils": "^0.7.4",
    "@iconify/json": "^2.2.87",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.16",
    "@vueuse/core": "^10.2.1",
    "@vueuse/metadata": "^10.2.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.3.0",
    "fs-extra": "^11.1.1",
    "fuse.js": "^6.6.2",
    "gray-matter": "^4.0.3",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "postcss": "^8.4.25",
    "postcss-nested": "^6.0.1",
    "rimraf": "^5.0.1",
    "simple-git": "^3.19.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unocss": "^0.53.4",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.32",
    "vitepress": "1.0.0-beta.5",
    "vitest": "^0.31.3",
    "vue": "^3.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
